@model List<ManagerDesk.ViewModels.BookingViewModel>

@if (Model.Any())
{
    <div class="row">
        @foreach (var book in Model)
        {
            <div class="col-md-6 col-lg-3">
                @Html.Partial("BookingCard", book)
            </div>
        }
    </div>
}
else
{
    <div class="row">
        <div class="col-md-12">
            <h1 class="login__heading">У вас пока нет заявок на бронирование</h1>
        </div>
    </div>
}