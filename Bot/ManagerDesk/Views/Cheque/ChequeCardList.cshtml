@model List<ManagerDesk.ViewModels.ChequeViewModel>

@if (Model.Any())
{
    <div class="row">
        @foreach (var cheque in Model)
        {
            <div class="col-md-6 col-lg-3">
                @Html.Partial("ChequeCard", cheque)
            </div>
        }
    </div>
}
else
{
    <div class="row">
        <div class="col-md-12">
            <h1 class="login__heading">У вас пока нет чеков</h1>
        </div>
    </div>
}