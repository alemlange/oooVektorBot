@model List<ManagerDesk.ViewModels.FeedbackViewModel>

@if (Model.Any())
{
    <div class="row">
        @foreach (var feed in Model)
        {
            <div class="col-md-6 col-lg-3">
                @Html.Partial("FeedbackCard", feed)
            </div>
        }
    </div>
}
else
{
    <div class="row">
        <div class="col-md-12">
            <h1 class="login__heading">У вас пока нет отзывов</h1>
        </div>
    </div>
}