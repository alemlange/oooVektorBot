@model List<ManagerDesk.ViewModels.DishListViewModel>
@foreach (var dishList in Model)
{
    <div class="row">
        <div class="col-md-12">
            <h1 class="login__heading">@dishList.CategoryString</h1>
        </div>
    </div>

    <div class="row">
        @foreach (var dish in dishList.Dishes)
        {
            <div class="col-md-6 col-lg-3">
                @Html.Partial("DishCard", dish)
            </div>
        }
    </div> 
}