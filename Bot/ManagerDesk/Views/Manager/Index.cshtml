@{
    Layout = "~/Templates/MainPageLayout.cshtml";
}

<div class="page-wrapper">
    <header class="header-top">
        <nav class="navbar-top">
            <div class="navbar-left-btn"><i class="fa fa-bars"></i></div>
            <a>Добавить</a>
            <a>Редактировать</a>
            <a>Удалить</a>
            <a class="logout"><i class="fa fa-sign-out"></i> Выйти</a>
        </nav>
    </header>
    @Html.Partial("Navigation")
    <div class="clearfix"></div>
    <section class="info-main">
        <div class="container-fluid">
            <div class="row js-table-body">
                @Html.Action("AllTables", "Manager")
            </div>
        </div>
    </section>
    <footer class="footer-bottom">
        <div class="copyright">Designed by VektorBot &copy; 2017</div>
    </footer>
</div> 


@section scripts
{
    <script src="@Url.Content("~/Assets/Scripts/main.js")" type="text/javascript"></script>
}